{
  "name": "template-vue-multi-pages",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --mode local",
    "serve:tests": "vue-cli-service serve --mode tests",
    "serve:prod": "vue-cli-service serve --mode production",
    "build:dev": "vue-cli-service build --mode development",
    "build:test": "vue-cli-service build --mode tests",
    "build:prod": "vue-cli-service build --mode production",
    "lintfix": "vue-cli-service lint --fix .js,.vue src",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "ajv": "^6.9.1",
    "axios": "^0.18.0",
    "glob": "^7.1.3",
    "vant": "^2.0.7",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-plugin-unit-mocha": "^3.0.0-rc.9",
    "@vue/cli-service": "^3.9.0",
    "@vue/component-compiler-utils": "^2.2.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.12.0",
    "chai": "^4.1.2",
    "ckeditor": "4.11.0",
    "crypto-js": "^3.1.9-1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "node-sass": "^4.10.0",
    "postcss-pxtorem": "^4.0.1",
    "sass": "^1.15.0",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "vue-eslint-parser": "^3.2.2",
    "vue-template-compiler": "^2.5.17",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-pxtorem": {
        "rootValue": 75,
        "propList": [
          "*"
        ],
        "selectorBlackList": [
          "van-"
        ],
        "minPixelValue": 12
      }
    }
  }
}
