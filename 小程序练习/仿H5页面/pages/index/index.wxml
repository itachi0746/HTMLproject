<!--index.wxml-->
<view>
  <view class="music">
    <!-- <image src="{{musicUrl}}"></image> -->
    
  </view>
  <swiper indicator-dots="{{indicatorDots}}"
    autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" vertical="{{vertical}}">
    <block wx:for="{{imgUrls}}">
      <swiper-item>
        <image src="{{item}}" class="slide-image" mode="{{imgMode}}"/>
      </swiper-item>
    </block>
  </swiper>

</view>
